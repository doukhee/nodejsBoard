<script>
  $(document).ready(function(){
    function AllCheck(){
      $("#AllCheck").click(function(){
        if($(this).is(':checked')){
          $("input[name=check]").prop("checked", true);
        }else{
          $("input[name=check]").prop("checked", false);
        }
      });
    }
    function Create(){
      $("#createBtn").click(function(){
        document.location.href="/admin/boards/create";

      });
    }
