<!DOCTYPE html>
<html>
<head>
  <%- include ("../../../partials/admin/include/stylesheet.ejs") %>
</head>
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">
    <%- include ("../../../partials/admin/include/header.ejs") %>
    <!-- Left side column. contains the logo and sidebar -->
    <%- include ("../../../partials/admin/include/LeftNav.ejs") %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Dashboard Member Create
        <small>Version 2.0</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Dashboard</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="box" style="margin:10% auto; width:70%;">
        <div class='box-header'>
          <h3 class="box-title">Registe User</h3>
        </div>
        <!--./box-header-->
        <div class="box-body">
          <form class="form-horizontal" name="memberRegisteForm" id="memberRegisteForm">
            <div class="form-group">
              <label for="email" class="control-label col-sm-2">User Email</label>
              <div class="col-sm-8">
                <input type="text" class="form-control" name="email" id="email">
              </div>
            </div>
            <!--./form-group-->
            <div class="form-group">
              <label for="password" class="col-sm-2 control-label">User Password</label>
              <div class="col-sm-8">
                <input type="password" name="password" id="password" class="form-control">
              </div>
            </div>
            <!--./form-group-->
            <div class="form-group">
              <label for="cpassword" class="col-sm-2 control-label">Confirm Password</label>
              <div class="col-sm-8">
                <input type="password" name="cpassword" id="cpassword" class="form-control">
              </div>
            </div>
            <!--./form-group-->
            <div class="form-group">
              <label for="name" class="control-label col-sm-2">User Name</label>
              <div class="col-sm-8">
                <input type="text" class="form-control" name="name" id="name">
              </div>
            </div>
            <!--./form-group-->
          </form>
          <!--./form-->
        </div>
        <!--./box-body-->
        <div class="box-footer">
          <div class='pull-left'>
            <button type="button" class="btn btn-flat btn-danger">Cancel</button>
          </div>
          <!--./pull-left-->
          <div class="pull-right">
            <button type="button" class="btn btn-flat btn-primary">Registe</button>
          </div>
          <!--./pull-right-->
        </div>
        <!--./box-footer-->
      </div>
      <!--./box-->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->

  <%- include ("../../../partials/admin/include/footer.ejs") %>

</div>
<!-- ./wrapper -->
<%- include ("../../../partials/admin/include/javascript.ejs") %>
</body>
<script>
  $(document).ready(function(){

  });
</script>
</html>
